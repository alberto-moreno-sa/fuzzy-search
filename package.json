{
  "name": "fuzzy-search",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "flow-bin": "^0.79.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react-app-rewired": "^1.5.2"
  },
  "scripts": {
    "start": "NODE_PATH=./src react-app-rewired start",
    "build": "NODE_PATH=./src react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "precommit": "yarn flow check && lint-staged",
    "flow": "flow",
    "lint": "prettier --write \"./**/*.{js,jsx,json,css,md}\"",
    "lint:check": "prettier --list-different \"./**/*.{js,jsx,json,css,md}\""
  },
  "lint-staged": {
    "./**/*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/src/app$1"
    }
  }
}
